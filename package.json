{
  "name": "intune-packager",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "lint": "eslint",
    "format": "npm run format:js && npm run format:rust",
    "format:js": "prettier . --write",
    "format:rust": "cargo fmt --manifest-path src-tauri/Cargo.toml --all",
    "clean": "npm run clean:js && npm run clean:rust",
    "clean:js": "rimraf node_modules dist build",
    "clean:rust": "cargo clean --manifest-path src-tauri/Cargo.toml",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.14",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-opener": "^2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tauri-apps/cli": "^2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.6.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.45.0",
    "vite": "^6.0.3"
  }
}
